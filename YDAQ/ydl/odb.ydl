package ODB is

   type Config is
      DBState : String;
   end Config;

   type DBBuffer is
      DIFID : Integer;
      PayLoad : Binary;
    end DBBuffer;
   
   type Status is
     oracleStatus: Integer;
    end Status;


   interface StateMachine is
      message Initialise(Res : out Status);
      message Download (Conf : in Config);
      message Dispatch(Res : out Status);
   end StateMachine;

end ODB;

